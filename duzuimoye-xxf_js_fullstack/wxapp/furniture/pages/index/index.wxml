<view class="page">
    <swiper class="section "indicator-dots="{{true}}" autoplay="autoplay" setInterval="3000" duration="500">
      <view class="img-box">
        <block wx:for="{{slides}}" wx:key="id">
          <swiper-item>
            <image src="{{item.image}}" mode="aspectFill" />
          </swiper-item>
        </block>
      </view>
    </swiper>
</view>